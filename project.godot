; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Another FNF engine made in Godot"
config/version=" "
run/main_scene="res://source/menus/title_menu/titleMenu.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/bg_color=Color(0.792157, 1, 0.301961, 1)
boot_splash/image="res://funkay.png"
config/icon="res://engine_icon.png"
run/vsync/vsync_mode=0

[autoload]

Conductor="*res://source/globals/Conductor.gd"
SoundStuff="*res://source/globals/SoundStuff.gd"
Global="*res://source/globals/Global.gd"
SongData="*res://source/globals/SongData.gd"
GlobalOptions="*res://source/globals/Global_Options.gd"
Flash="*res://source/globals/Flash.tscn"
Transition="*res://source/globals/transition.tscn"
Volume="*res://source/globals/volume.tscn"
Fps="*res://source/globals/FPS.tscn"
ScreenShootMenu="*res://source/globals/Screen_shoot_menu.tscn"
MusicManager="*res://source/globals/Music_Manager.gd"
Discord="*res://source/globals/Discord.gd"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"
HighScore="*res://source/globals/HighScore.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://assets/images/mouse.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-rpc-gd/plugin.cfg", "res://addons/xml_converter/plugin.cfg")

